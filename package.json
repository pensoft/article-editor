  "scripts": {
    "ng": "ng",
    "config": "ts-node ./setenv.ts",
    "serve": "npm run config -- --environment=dev && ng serve --proxy-config proxy.conf.js --open",
    "start": "npm run config -- --environment=dev && ng serve --proxy-config proxy.conf.js --open",
    "startBuild": "http-server -p 8080 -c-1 dist/article-editor ",
